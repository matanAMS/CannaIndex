<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ams">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/weed_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/weed_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ForgetPass"></activity>
        <activity android:name=".CannabisMethods" />
        <activity android:name=".WeedInfo" />
        <activity android:name=".StrainListView" />
        <activity android:name=".ShowDataTest" />
        <activity android:name=".CompaniesListView" />
        <activity android:name=".StrainFormActivity" />
        <activity
            android:name=".WelcomeNavActivity"
            android:label="@string/title_activity_welcome_nav"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".WelcomeActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <service
            android:name=".FirebaseServiceID">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
    </application>

</manifest>